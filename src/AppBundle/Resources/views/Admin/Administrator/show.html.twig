{% extends 'AppBundle:Admin:layoutAdmin.html.twig' %}

{% block body %}
<div class="admin page-wrap">
    <div class="wc-title-banner">
        <h2>Administrator</h2>
    </div>

    <div class="wc-container">
        <div class="col-sm-12">
                {{ dump(administrator) }}
            <div class="wc-admin-block">
                <div class="wc-admin-row">
                    <div class="row">
                        <table class="table table-striped">
                            <tr>
                                <td>
                                    <a href="{{ path('admin.administrator.delete', {'administratorId': administrator.id}) }}"><button type="button" class="btn-admin-form btn-modify" name="button">Supprimer</button></a>

                                </td>
                                <td>
                                    <a href="{{ path('admin.administrator.list') }}"><button type="button" class="btn-admin-form btn-modify" name="button">Retour liste</button></a>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{% endblock %}
